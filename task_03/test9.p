        album : integer;
        brocoli, cicory : integer;
        drova, eley, grOB : integer;
    begin
        begin
            album := 12;
        end;
        begin
            brocoli := 1 + 2 + 3 + 4 + 5 + 16 + 17 + 18 + 19 + 20 + 300 + 400 + 500 + 600 + 0700 + 800;
            cicory := brocoli div 2;
        end;
        drova := album * brocoli - cicory + 1337;
